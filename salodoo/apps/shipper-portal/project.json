{
  "name": "shipper-portal",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/shipper-portal/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/shipper-portal"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nrwl/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "shipper-portal:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "shipper-portal:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "shipper-portal:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nrwl/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "shipper-portal:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "shipper-portal:build:development"
        },
        "production": {
          "buildTarget": "shipper-portal:build:production"
        }
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": [
        "coverage/apps/shipper-portal"
      ],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/shipper-portal"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/shipper-portal/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "component-test": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/shipper-portal/cypress.config.ts",
        "testingType": "component",
        "devServerTarget": "shipper-portal:build",
        "skipServe": true
      }
    }
  },
  "tags": []
}
